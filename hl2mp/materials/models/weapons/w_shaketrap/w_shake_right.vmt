"UnLitGeneric"
{
	// Original shader: VertexLitMaskedEnvMappedTexture
	"$basetexture" "models\weapons\w_shaketrap\w_shake_right"

        "$envmapmask" "models\weapons\w_shaketrap\w_shake_right_mask"
	

       	"$envmap" "env_cubemap"
	"$envmapsaturation" "[1 1 1]"
	"$envmapcontrast" 1
	"$surfaceprop" "flesh"
	
//	"$additive" "1"
//	"$translucent" "1"
//	"$nocull" "1"
	"$model" "1"

"Proxies"
{
 "AnimatedTexture"
 {
  "animatedtexturevar" "$basetexture"
  "animatedtextureframenumvar" "$frame"
  "animatedtextureframerate" 15 
 }
}

}

